{
  "version": 2,
  "framework": null,
  "buildCommand": "",
  "devCommand": "",
  "installCommand": "npm install --registry=https://registry.npmjs.org/",
  "functions": {
    "api/**/*.js": {
      "memory": 256,
      "maxDuration": 30
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
